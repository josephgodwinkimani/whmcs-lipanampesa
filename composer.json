{
    "name": "josephgodwinkimani/whmcs-lipanampesa",
    "type": "library",
    "description": "Accept WHMCS Payments using Lipa na M-PESA",
    "keywords": [
        "safaricom",
        "whmcs",
        "web host automation platform",
        "lipa na mpesa",
        "gateway module",
        "third party gateway"
    ],
    "homepage": "https://github.com/josephgodwinkimani/whmcs-lipanampesa/",
    "license": "MIT",
    "authors": [
        {
            "name": "Joseph Godwin Kimani",
            "email": "josephgodwinke@gmail.com",
            "homepage": "https://kimani.gocho.live/",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "josephgodwinke@gmail.com",
        "wiki": "https://github.com/josephgodwinkimani/whmcs-lipanampesa/wiki"
    },
    "require": {
        "php": "^7.2 || ^8.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^8.0 || ^9.0",
        "vimeo/psalm": "^4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Example\\": "modules/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Example\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": [
            "@php ./vendor/bin/phpunit"
        ]
    }
}
